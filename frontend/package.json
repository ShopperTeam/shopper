{
    "name": "frontend",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "storybook": "ng run frontend:storybook",
        "build-storybook": "ng run frontend:build-storybook",
        "compodoc": "npx compodoc -p tsconfig.doc.json -w -s",
        "lint": "ng lint",
        "format": "npx prettier . -c --ignore-path .gitignore",
        "format:fix": "npx prettier . -w --ignore-path .gitignore",
        "prepare": "cd .. && husky install frontend/.husky"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.0.2",
        "@angular/common": "^17.0.2",
        "@angular/compiler": "^17.0.2",
        "@angular/core": "^17.0.2",
        "@angular/forms": "^17.0.2",
        "@angular/platform-browser": "^17.0.2",
        "@angular/platform-browser-dynamic": "^17.0.2",
        "@angular/router": "^17.0.2",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "flowbite": "^1.8.1",
        "prettier-eslint": "^16.1.1",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.0.0",
        "@angular-eslint/builder": "17.0.1",
        "@angular-eslint/eslint-plugin": "17.0.1",
        "@angular-eslint/eslint-plugin-template": "17.0.1",
        "@angular-eslint/schematics": "17.0.1",
        "@angular-eslint/template-parser": "17.0.1",
        "@angular/compiler-cli": "^17.0.2",
        "@angular/language-service": "^17.0.2",
        "@compodoc/compodoc": "^1.1.21",
        "@storybook/addon-docs": "^7.4.5",
        "@storybook/addon-essentials": "^7.4.5",
        "@storybook/addon-interactions": "^7.4.5",
        "@storybook/addon-links": "^7.4.5",
        "@storybook/angular": "^7.4.5",
        "@storybook/blocks": "^7.4.5",
        "@storybook/testing-library": "^0.2.2",
        "@tailwindcss/typography": "^0.5.10",
        "@types/jasmine": "~4.3.0",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.52.0",
        "eslint-plugin-storybook": "^0.6.15",
        "husky": "^8.0.3",
        "jasmine-core": "~4.6.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "lint-staged": "^15.0.2",
        "postcss": "^8.4.30",
        "prettier": "^3.0.3",
        "prettier-plugin-organize-imports": "^3.2.3",
        "prettier-plugin-tailwindcss": "^0.5.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "storybook": "^7.4.5",
        "tailwindcss": "^3.3.3",
        "typescript": "~5.2.2"
    },
    "lint-staged": {
        "*.{ts,html}": [
            "eslint --quiet --fix"
        ],
        "*.{json,md,html,ts}": [
            "prettier --write"
        ]
    }
}
